{
  "name": "discord-scheduler",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "deploy": "wrangler deploy",
    "predeploy": "npm run register",
    "register": "tsx --env-file=.env ./src/register.ts",
    "dev": "wrangler dev",
    "start": "wrangler dev",
    "test": "vitest",
    "generate:drizzle": "drizzle-kit generate",
    "migrate:local": "wrangler d1 migrations apply discord-scheduler --local",
    "migrate:remote": "wrangler d1 migrations apply discord-scheduler --remote",
    "drizzle-studio": "drizzle-kit studio --config ./drizzle-studio.config.ts"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@cloudflare/vitest-pool-workers": "^0.5.41",
    "@cloudflare/workers-types": "^4.20250303.0",
    "@libsql/client": "^0.14.0",
    "@mst-mkt/js-config": "npm:@jsr/mst-mkt__js-config@^0.1.1",
    "@types/node": "^22.13.10",
    "discord-api-types": "^0.37.119",
    "drizzle-kit": "^0.30.5",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2",
    "vitest": "2.1.8",
    "wrangler": "^3.114.0"
  },
  "dependencies": {
    "discord-hono": "^0.16.3",
    "drizzle-orm": "^0.40.0",
    "neverthrow": "^8.2.0",
    "ts-pattern": "^5.6.2"
  }
}
